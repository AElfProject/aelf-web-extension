{
  "manifest_version": 3,
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "name": "aelf-explorer-extension",
  "version": "1.2.9",
  "description": "__MSG_pluginDesc__",
  "icons": {
    "16": "assets/images/aelf_logo.png",
    "48": "assets/images/aelf_logo.png",
    "128": "assets/images/aelf_logo.png"
  },
  "background": {
    "service_worker": "./js/background.js"
  },
  "action": {
    "default_icon": "assets/images/aelf_logo.png",
    "default_title": "aelf-explorer-extension",
    "default_popup": "popup.html"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "app", "windows", "tabs", "scripting"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["js/inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html"
  },
  "default_locale": "zh_CN"
}
